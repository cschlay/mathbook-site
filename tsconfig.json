{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "es2020",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": false,
    "sourceMap": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true
  }
}
